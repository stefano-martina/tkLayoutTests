// Second level conversion rules

Station {
  stationName endcap1
  type second

  minZ 1500
  maxZ 1600
  Conversion {
    Input {
      Element {
        elementName Cu
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName Cu
        quantity 1
        unit g
      }
    }
  }
}

destination endcap1
