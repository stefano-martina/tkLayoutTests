// Second level conversion rules

Station {
  stationName endcap2
  type second

  minZ 2000
  maxZ 2100
  Conversion {
    Input {
      Element {
        elementName Cu
        quantity 1
        unit g/m
      }
    }
    Output {
      Element {
        elementName Cu
        quantity 1
        unit g
      }
    }
  }
}
